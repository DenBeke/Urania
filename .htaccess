Allow from All
RewriteEngine on

SetEnv DEVELOPMENT_ENVIRONMENT true

#Exclude all directories
RewriteCond %{REQUEST_FILENAME} -f  [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule .*  - [L]

#One url parameter
RewriteRule ^([a-zA-Z0-9_\-]*)/$ index.php?page=$1
RewriteRule ^([a-zA-Z0-9_\-]*)$ index.php?page=$1

#Two url parameters
RewriteRule ^([a-zA-Z0-9_\-]*)/([a-zA-Z0-9_\-]*)$ index.php?page=$1&album=$2&image=$2
RewriteRule ^([a-zA-Z0-9_\-]*)/([a-zA-Z0-9_\-]*)/$ index.php?page=$1&album=$2&image=$2

#Three url parameters
RewriteRule ^([a-zA-Z0-9_\-]*)/([a-zA-Z0-9_\-]*)/([a-zA-Z0-9_\-]*)$ index.php?page=$1&album=$2&image=$2
RewriteRule ^([a-zA-Z0-9_\-]*)/([a-zA-Z0-9_\-]*)/([a-zA-Z0-9_\-]*)/$ index.php?page=$1&album=$2&image=$2